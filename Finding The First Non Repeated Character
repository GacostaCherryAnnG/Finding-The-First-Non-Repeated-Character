package com.example;

public class Findingthefirstnonrepeatedcharacter {

 public static void main(String[] args) {
 String s = "cherry ann";
 for(int i=0;i<s.length();i++) {
   int count =0;
   for(int c=0;c<s.length();c++) {
     if(s.charAt(i)==s.charAt(c)){
       count++;
     }
   }
   if(count==1){
   System.out.println("First non repeating char is " + s.charAt(i));
     break;
   }
 }
